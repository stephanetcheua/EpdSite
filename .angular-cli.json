{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "casino-site-ui"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "api",
        "assets/fonts",
        "assets/img",
        "assets/medical",
        "assets/wetzlar",
        "favicon.ico",
        "js/device.min.js",
        "js/superfish.js",
        "js/jquery.mousewheel.min.js",
        "js/jquery.simplr.smoothscroll.min.js",
        "js/stellar/jquery.stellar.js",
        "js/jquery.ui.totop.js",
        "web.config",
        ".htaccess"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "./css/styles.scss",
        "./css/bootstrap.scss",
        "./css/style.scss",
        "./css/animate.scss",
        "./css/camera.scss",
        "./css/contact-form.scss",
        "./css/touchTouch.scss",
        "./assets/fonts/font-awesome.css"
      ],
      "scripts": [
        "./js/jquery.js",
        "./js/jquery-migrate-1.2.1.min.js",
        "./js/jquery.easing.1.3.js",
        "./js/jquery.equalheights.js",
        "./js/TMForm.js",
        "./js/bootstrap-filestyle.js",
        "./js/jquery.mobile.customized.min.js",
        "./js/jquery.touchSwipe.min.js",
        "./js/wow/wow.js",
        "./js/html5shiv.js",
        "./js/respond.min.js",
        "./js/bootstrap.min.js",
        "./js/tm-scripts.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
